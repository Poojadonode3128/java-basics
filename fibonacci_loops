import java.util.*;
public class Main
{   public static void fibonacci(int n){
    int a=0;
    int b=1;
    int c=1;
    for(int i=0;i<=n;i++){
        System.out.print(a+" ");
        a=b;
        b=c;
        c=a+b;
    }
    
}
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
		fibonacci(n);
	}
}
