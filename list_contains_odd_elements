import java.util.*;
public class Main

{   
    public static boolean OddListElements(List<Integer> list)
    {
        for(int i:list){
            if(i%2==0){
                return false;
            }
        }
        return true;
    }
	public static void main(String[] args) {
	    List<Integer> list=new ArrayList<>();
	    Scanner sc=new Scanner(System.in);
	    while(sc.hasNext()){
	        list.add(sc.nextInt());
	        break;
	    }
	    System.out.println(OddListElements(list));
	}
}
